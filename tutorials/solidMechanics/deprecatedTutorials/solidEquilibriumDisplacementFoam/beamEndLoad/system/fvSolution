FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    D GAMG
    {
        solver           GAMG;
        smoother         GaussSeidel;

        tolerance        1e-06;
        relTol           0.99;

        cacheAgglomeration true;

        nCellsInCoarsestLevel 6;

        agglomerator     faceAreaPair;
        mergeLevels      1;
    }

    Dcorr GAMG
    {
        solver           GAMG;
        smoother         GaussSeidel;

        tolerance        1e-06;
        relTol           0.99;

        cacheAgglomeration true;

        nCellsInCoarsestLevel 6;

        agglomerator     faceAreaPair;
        mergeLevels      1;
    }
}

stressAnalysis
{
    nCorrectors        1;
    D                  1e-10;
    accelerationFactor 2;
}

