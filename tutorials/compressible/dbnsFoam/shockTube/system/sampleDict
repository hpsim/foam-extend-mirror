FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cell;

setFormat     raw;

sets
(
    uniform
    {
        type            uniform;
        name            cut;
        axis            distance;
        start           (0.001 0.05 0.05);
        end             (0.999 0.05 0.05);
        nPoints         100;
    }
);

fields
(
    rho
);


