FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 0
);

regions
(
    sphereToCell
    {
        centre (0.0002 0 0);
        radius 1.2e-4;
        fieldValues
        (
            volScalarFieldValue T 1
        );
    }
);

